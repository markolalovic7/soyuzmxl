    "slimmobile-lint": "env-cmd -f .env.development eslint --ext .js ./src/applications/slimmobile",
    "start": "env-cmd -f .env.development react-scripts start",
    "postinstall": "patch-package",
    "vanilla-lint": "env-cmd -f .env.development eslint --ext .js ./src/applications/vanillamobile",
    "scripts-sort-translations": "babel-node ./scripts/scripts-sort-translations.js",
    "test": "mocha ./src/__tests__ --recursive --require @babel/register",
